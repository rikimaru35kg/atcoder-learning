      "        return x;",
      "    }",
      "    mints &operator*=(mints x) { *this = *this * x; return *this; }",
      "    mints operator*(long long x) const { return *this * mints(x); }",
      "    mints pow(long long x) const {",
      "        if (x==0) return mints(1);",
