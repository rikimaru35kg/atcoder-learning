      "            return data[i] < x.data[i];",
      "        }",
      "        return false;",
      "    }",
      "    bool operator==(mints x) const {",
      "        for(int i=0; i<MX; ++i) if (data[i] != x.data[i]) return false;",
      "        return true;",
      "    }",
      "    void print() const {",
