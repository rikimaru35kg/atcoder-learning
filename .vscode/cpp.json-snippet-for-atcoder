      "    vector<long long> ret;",
      "    for(long long i=1; i*i<=x; ++i) {",
      "        if (x % i == 0) {",
      "            ret.push_back(i);",
      "            if (i*i != x) ret.push_back(x / i);",
      "        }",
      "    }",
      "    if (issort) sort(ret.begin(), ret.end());",
      "    return ret;",
      "}",
    ],
